{
  "name": "travelama-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "kill": "for port in 3000 3001 3002 3003 3004 3005; do pids=$(lsof -ti :$port 2>/dev/null); if [ -n \"$pids\" ]; then echo \"Killing processes on port $port: $pids\"; kill $pids 2>/dev/null; sleep 1; kill -9 $pids 2>/dev/null; fi; done; echo 'Ports cleared'",
    "predev": "npm run kill",
    "dev": "next dev --turbopack -p 3000",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db": "supabase stop --all && supabase start",
    "db:stop": "supabase stop"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-email/render": "^1.4.0",
    "@react-google-maps/api": "^2.20.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tailwindcss/typography": "^0.5.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.562.0",
    "next": "^16.1.0",
    "next-themes": "^0.4.6",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "resend": "^6.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
